{
    "routes": [
        {
            "route": "/admin",
            "redirect": "/index.html"
        },
        {
            "route": "/{shortUrl}",
            "rewrite": "/api/urlredirect/{shortUrl}"
        },
        {
            "route": "/api/*",
            "rewrite": "/api/urlredirect"
        },
        {
            "route": "/api/{shortUrl}",
            "rewrite": "/api/urlredirect/{shortUrl}"
        }
    ]
}